package chapter10;

import java.text.DateFormat;
import java.text.SimpleDateFormat;
import java.util.Calendar;
import java.util.Date;

public class Main {

	public static void main(String[] args) {
		//Value v1 = new Value (10);
		//Value v2 = new Value (10);
		//v1=v2;
		
		//if(v1.equals(v2)) {
		//	System.out.println("v1 == v2");
		//} else { 
		//	System.out.println("v1 != v2");
		//}
		
		Person p1 = new Person("김수현");
		Person p2 = new Person("김수현");
		
		if(p1.equals(p2)) {
			System.out.println("p1 == p2");
		} else { 
			System.out.println("p1 != p2");
		}
		if(p1==p2) {
			System.out.println("p1 == p2");
		} else { 
			System.out.println("p1 != p2");
		}
		
		Card card1 = new Card("spade",3);
		Card card2 = new Card("spade",3);
		
		System.out.println(card1.toString());
		System.out.println(card2.toString());
		
		Date now = new Date();
		String strnow = now.toString();
		System.out.println(strnow);
		
		SimpleDateFormat sdf = new SimpleDateFormat("YYYY년 MM월 dd일 hh시 mm분 ss초"); 
		String strnow2 = sdf.format(now);		
		System.out.println(strnow2);
		
		Calendar cal = Calendar.getInstance();
		int year = cal.get(Calendar.YEAR);
		int month = cal.get(Calendar.MONTH)+1;
		int day = cal.get(Calendar.DAY_OF_MONTH);
		int hour = cal.get(Calendar.HOUR);
		int minute = cal.get(Calendar.MINUTE)+1;
		int second = cal.get(Calendar.SECOND)+1;
		
		System.out.printf("%년%월%일%시%분%초"+year,month,day,hour,minute,second);
	}

}
