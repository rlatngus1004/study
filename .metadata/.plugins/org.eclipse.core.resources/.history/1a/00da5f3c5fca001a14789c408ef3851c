package chapter12;

import java.util.HashSet;
import java.util.Set;

public class Main2 {

	public static void main(String[] args) {
		Set<String> names = new HashSet<>();
		
		names.add("고");
		names.add("김");
		names.add("박");
		names.add("김");
		names.add("이");
		
		System.out.println(names.size());
		
		names.remove("김");
		
		System.out.println(names.size());
		

	}

}
