package chapter02;

public class book {

	public static void main(String[] args) {
		int[][] array = {
				{95,86},{83,92,96},{78,83,93,87,88}
				};
		for (int i=0;i<array.length;i++) {
			array[i];
			System.out.printf("array.length = %d" ,+i);
		}
		for (int j=0;j<array.length;j++) {
			array[2][j];
			System.out.printf("array.length = %d" ,+j);
		}

					
	}

}
