package chapter02;

public class book3 {

	public static void main(String[] args) {
		int[] total = new int [] {};
		int sum = 0;
		int[][] array = {
				{95,86},
				{83,92,96},
				{78,83,93,87,88}
				};
		for (int j=0; j<array.length; j++) {
			total = array[j];
		}
		for(int k=0;k<total.length;k++) {
			sum = sum + total[k];
		}
		double avg = sum/10;
		System.out.printf("d%,d%"+sum,+avg);
		
	
				
		

}
}