package chapter09;

public class Main3 {

	public static void main(String[] args) {
		int[] num = {1,2,3};
		try {
			System.out.println(num[5]/0);
		}catch(ArrayIndexOutOfBoundsException err1) {
			System.out.println("인덱스 범위를 벗어났습니다.");
		}
		catch(ArithmeticException err2) {
			System.out.println("0으로 나눌 수 없습니다.");
		}

	}

}
