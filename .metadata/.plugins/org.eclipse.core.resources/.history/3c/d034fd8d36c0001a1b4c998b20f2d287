}
		public member(String name) {
			this.id= id;
		}
		public member(String id) {
		}
		public member(String password) {
		}
		public member(int age) {			
		}
		public void prt() {
			System.out.println("*******************************");
			System.out.println("차량모델: "+this.name);
			System.out.println("차량색상: "+this.color);
