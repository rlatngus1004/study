package chapter05;

public class SignIn {
	
	System.out.println("아이디를 입력하시요");
	String id = scan.nextInt();
	System.out.println("비밀번호를 입력하시요.");
	String pwd = scan.nextInt();
	System.out.println("이름을 입력하시요.");
	String name = scan.nextInt();
	System.out.println("전화번호를 입력하시요.");
	String phone = scan.nextInt();
	Member member = new Member(id,pwd,name,phone);
	return member;
}
