package chapter02;

import java.util.Random;

public class ex01 {

	public static void main(String[] args) {
		Random R = new Random();
		int dice1 = R.nextInt(45)+1;
		int dice2 = R.nextInt(45)+1;
		int dice3 = R.nextInt(45)+1;
		int dice4 = R.nextInt(45)+1;
		int dice5 = R.nextInt(45)+1;
		int [] x;
		x = new int[]{dice1,dice2,dice3,dice4,dice5};
			System.out.printf("%d,%d,%d,%d,%d", x[0],x[1],x[2],x[3],x[4]);
 		
	}
	

}
