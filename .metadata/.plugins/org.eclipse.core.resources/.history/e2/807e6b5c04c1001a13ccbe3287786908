package chapter04;

public class Member {
	public void id(String id) {
		this.name = name
	}
	public void password(String password){
		this.name = name
		this.name = name
	}
	public void name(String name) {
		this.name = name
	}
	public void age(String int age) {
		this.name = name
	}
	

	String name = "이름";//필드
	String id  = "아이디";
	String password = "비밀번호";
	int age = 0;
}
